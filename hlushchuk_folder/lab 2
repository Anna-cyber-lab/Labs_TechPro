CODE 1:
let d1, d2; 

  

while (true) { 

  const startDateStr = prompt("Введіть початкову дату у форматі YYYY-MM-DD: "); 

  d1 = new Date(startDateStr); 

  if (!isNaN(d1)) break;  

  console.log("Некоректна дата! Спробуйте ще раз."); 

} 

  

while (true) { 

  const endDateStr = prompt("Введіть кінцеву дату у форматі YYYY-MM-DD: "); 

  d2 = new Date(endDateStr); 

  if (isNaN(d2)) { 

    console.log("Некоректна дата! Спробуйте ще раз."); 

  } else if (d2 < d1) { 

    console.log("Кінцева дата не може бути раніше початкової! Спробуйте ще раз."); 

  } else { 

    break;  

  } 

} 

  

let diff = d2 - d1; 

let minutes = diff / (1000 * 60);  

console.log("Різниця в хвилинах:", minutes); 

console.log(typeof (d1));








CODE 2:

const arr = []; 

const length = 10; 

 

for (let i = 0; i < length; i++) { 

  const randomNum = Math.floor(Math.random() * 100); 

  arr.push(randomNum); 

} 

console.log("Згенерований масив: ", arr); 

 

let valid = true; 

for (let num of arr){ 

    if ( typeof num !== "number" || isNaN(num)){ 

        valid = false; 

        break; 

    } 

} 

if (!valid){ 

    console.log("Всі елементи масиву повинні бути числами!") 

}else{ 

 

 

 

let min = arr[0]; 

let count = 1; 

let i = 1; 

 

while (i < arr.length) { 

  if (arr[i] < min) { 

    min = arr[i]; 

    count = 1; 

  } else if (arr[i] === min) { 

    count++; 

  } 

  i++; 

} 

 

 

console.log("Мінімальний елемент:", min); 

console.log("Кількість входжень мінімального елемента:", count); 

} 

console.log(typeof (d2)); 
